<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("Content/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
</head>
<body>
    <!--start navbar-->
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container-fluid">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar">
                    </span>
                </button>
                <a class="brand" href="#">Project name</a>
                <div class="nav-collapse collapse">
                    <p class="navbar-text pull-right">
                        Logged in as <a href="#" class="navbar-link">Username</a>
                    </p>
                    <ul class="nav">
                        <li class="active"><a href="#">Home</a></li>
                        <li><a href="#about">About</a></li>
                        <li><a href="#contact">Contact</a></li>
                    </ul>
                </div>
                <!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <!--end navbar-->
    <hr />
    <!--main-->
    <div class="container-fluid">
        <div class="row-fluid">
            <!--start left-->
            <div class="span2">
                <div class="well sidebar-nav">
                    <ul class="nav nav-list">
                        <li class="nav-header">SEO信息查询</li>
                        <li>@Html.ActionLink("百度搜索引擎", "/Baidu")</li>
                        <li>@Html.ActionLink("Google搜索引擎", "/Google")</li>
                        <li>@Html.ActionLink("Yahoo搜索引擎", "/Yahoo")</li>
                        <li>@Html.ActionLink("Sogou搜索引擎", "/Sogou")</li>
                        <li>@Html.ActionLink("Soso搜索引擎", "/Soso")</li>
                        <li>@Html.ActionLink("Bing搜索引擎", "/Bing")</li>
                        <li>@Html.ActionLink("Youdao搜索引擎", "/Youdao")</li>
                        <li>@Html.ActionLink("所有搜索引擎", "/AllEngine")</li>
                        <li class="nav-header">网站提交入口</li>
                        <li>@Html.ActionLink("百度提交入口", "#")</li>
                        <li>@Html.ActionLink("Google提交入口", "#")</li>
                        <li>@Html.ActionLink("Yahoo提交入口", "#")</li>
                        <li>@Html.ActionLink("Sogou提交入口", "#")</li>
                        <li>@Html.ActionLink("Soso提交入口", "#")</li>
                        <li>@Html.ActionLink("Bing提交入口", "#")</li>
                        <li>@Html.ActionLink("Youdao提交入口", "#")</li>
                    </ul>
                </div>
            </div>
            <!--end left-->
            <!--start right-->
            <div class="span9">
                @RenderBody()
            </div>
            <!--end right-->
        </div>
        <hr />
        <!--start footer-->
        <footer>
            <div style="text-align:center; color:gray;">
                网站SEO查询
            </div>
      </footer>
       <!--end footer-->
    </div>
    <!--end main-->
   
</body>
</html>
